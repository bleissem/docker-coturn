# what to start 
# 'nothing' = stun and turn
#  --stun-only = only stun
#  --no-stun = only turn
WHATTOSTART=

#user infos
USER=user
PASSWORD=password
REALM=realm

# external IP
EXTERNALIP=0.0.0.0

# certificate location
CERTDIR=./

# letsencrypt host and e-mail contact 
HOST=host
EMAIL=email

# port 
PORT=3478

# tls-listening-port
TLSLISTENINGPORT=5349

# min port
MINPORT=49152

# max-port
MAXPORT=65535

# use together with https://github.com/bleissem/docker-compose-letsencrypt-nginx-proxy-companion
NETWORK=webproxy